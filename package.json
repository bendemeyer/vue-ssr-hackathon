{
  "private": true,
  "name": "vue-ssr-hackathon",
  "version": "1.0.1",
  "scripts": {
    "start": "node dist/server/bundle.js",
    "start:dev": "mkdir -p dist/server && touch dist/server/bundle.js && nodemon --inspect dist/server/bundle.js",
    "start:api": "nodemon src/api.js",
    "build": "webpack --mode production --config webpack.config.js",
    "build:dev": "webpack --mode development --config webpack.config.js",
    "build:watch": "webpack --mode development --watch --config webpack.config.js",
    "dev": "concurrently npm:build:watch npm:start:dev",
    "test": "jest"
  },
  "dependencies": {
    "@fastify/static": "^6.5.0",
    "@vueuse/head": "^0.9.8",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "fastify": "^4.8.1",
    "lodash": "^4.17.21",
    "pinia": "^2.0.23",
    "uuid-js": "^0.7.5",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.0",
    "css-loader": "^6.7.1",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "null-loader": "^4.0.1",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.7.8",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
